# Deployed server specification
# Example: 1. 4c8g (4 CPU cores, 8GB RAM), 2. 8c16g (8 CPU cores, 16GB RAM) or higher
OS_RESOURCE_SPEC=2

# Operating system name
OS_NAME=supOS

# Path for storing persistent data
VOLUMES_PATH=/volumes/supos/data

# Protocol used for the service entrance (e.g., http or https)
ENTRANCE_PROTOCOL=http

# Domain or IP address for accessing the service
ENTRANCE_DOMAIN=127.0.0.1

# Port number for accessing the service
ENTRANCE_PORT=8088

# Language setting (supported: en-US, zh-CN)
LANGUAGE=en-US

# Operating system platform type (supported: windows, linux)
OS_PLATFORM_TYPE=linux

# Operating system version
OS_VERSION=V1.00.00.05-C

# Type of large language model used
OS_LLM_TYPE=ollama

# MQTT (Message Queuing Telemetry Transport) TCP port
OS_MQTT_TCP_PORT=1883

# MQTT WebSocket TLS (Transport Layer Security) port
OS_MQTT_WEBSOCKET_TSL_PORT=8084

# Login path for the system
OS_LOGIN_PATH=/supos-login

# Enable or disable authentication (true/false)
OS_AUTH_ENABLE=false

# OpenAI API model used
OPENAI_API_MODEL=gpt-4o

# API key for OpenAI (replace with actual key)
OPENAI_API_KEY=guess

# API key for LangSmith (optional)
LANGSMITH_API_KEY=

# Deployment URL for AI agents (if applicable)
AGENT_DEPLOYMENT_URL=

# API key for supOS (if applicable)
SUPOS_API_KEY=

# API URL for supOS services
SUPOS_API_URL=

# If MULTIPLE_TOPIC is true, topics prefixed with '_rs/' will be publicly exposed instead of the original topics
MULTIPLE_TOPIC=false

#--------------------OAUTH CONFIG------------------
# OAuth realm name
OAUTH_REALM=supos

# OAuth client name
OAUTH_CLIENT_NAME=supos

# OAuth client ID
OAUTH_CLIENT_ID=supos

# OAuth client secret (keep this secure)
OAUTH_CLIENT_SECRET=VaOS2makbDhJJsLlYPt4Wl87bo9VzXiO

# OAuth grant type (authorization_code is commonly used for authentication flows)
OAUTH_GRANT_TYPE=authorization_code

# OAuth issuer URI (URL of the Keycloak authentication service)
OAUTH_ISSUER_URI=http://keycloak:8080

# Token refresh interval (in minutes)
OAUTH_REFRESH_TOKEN_TIME=60

# Elasticsearch version used
ELASTICSEARCH_VERSION=7.10.2

# Elasticsearch time horizon (likely in days, affects data retention)
ELASTICSEARCH_TIMEHORIZON=60
